  "description": "Plugin for gemini which is intended to aggregate the results of tests running into html report",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha test",
    "precommit": "npm run lint",
    "prepublish": "webpack",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-testing/html-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-testing/html-reporter/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "gemini",
    "plugin",
    "html-reporter"
  ],
  "homepage": "https://github.com/gemini-testing/html-reporter#readme",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "clipboard": "^1.7.1",
    "fs-extra": "^2.0.0",
    "gemini-configparser": "^0.4.0",
    "lodash": "^4.17.4",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.5.0",
    "conventional-changelog-lint": "^1.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^3.4.0",
    "eslint-config-gemini-testing": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "husky": "^0.11.9",
    "mocha": "^3.0.2",
    "proxyquire": "^1.7.11",
    "qemitter": "^1.0.0",
    "sinon": "^4.0.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-clean": "^1.0.0",
    "webpack-handlebars-precompiler": "^1.0.0"
  }
