{
  "name": "html-reporter",
  "version": "2.0.0",
  "description": "Plugin for gemini and hermione which is intended to aggregate the results of tests running into html report",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha test",
    "precommit": "npm run lint",
    "prepublish": "NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "webpack-dev-server --config=webpack.dev.js --progress --colors",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-testing/html-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-testing/html-reporter/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "gemini",
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "homepage": "https://github.com/gemini-testing/html-reporter#readme",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "fs-extra": "^2.0.0",
    "gemini-configparser": "^1.0.0",
    "lodash": "^4.17.4",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "conventional-changelog-lint": "^1.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-gemini-testing": "^2.5.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-react-parser": "^0.4.0",
    "husky": "^0.11.9",
    "mocha": "^3.0.2",
    "prop-types": "^15.6.0",
    "proxyquire": "^1.7.11",
    "qemitter": "^1.0.0",
    "react": "^16.2.0",
    "react-clipboard.js": "^1.1.2",
    "react-dom": "^16.0.0",
    "react-lazy-load": "^3.0.13",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "sinon": "^4.0.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.1"
  }
}
